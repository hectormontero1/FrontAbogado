{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "cache": {
      "enabled": false
    },
    "analytics": "35c2a030-70e4-44b0-bcac-3c33ef95e5a5"
  },
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "velzon": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "baseHref": "/",
            "outputPath": "dist/velzon",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "node_modules/shepherd.js/dist/css/shepherd.css",
              "node_modules/flatpickr/dist/flatpickr.css",
              "src/styles.scss",
              "node_modules/@ctrl/ngx-emoji-mart/picker.css",
              "node_modules/swiper/swiper-bundle.css",
              "src/assets/scss/config/default/bootstrap.scss",
              "src/assets/scss/config/default/app.scss",
             "node_modules/ngx-spinner/animations/ball-scale-multiple.css",
               "node_modules/devextreme/dist/css/dx.light.css",
              "src/assets/scss/icons.scss"
              
            ],
            "scripts":[ ],
            "allowedCommonJsDependencies": [
              "ev-emitter/ev-emitter",
              "can-use-dom",
              "core-js/modules/es.array.filter.js",
              "core-js/modules/es.array.iterator.js",
              "core-js/modules/es.array.reduce.js",
              "core-js/modules/es.function.name.js",
              "core-js/modules/es.object.assign.js",
              "core-js/modules/es.object.to-string.js",
              "core-js/modules/es.parse-int.js",
              "core-js/modules/es.regexp.exec.js",
              "core-js/modules/es.string.iterator.js",
              "core-js/modules/es.string.match.js",
              "core-js/modules/es.string.replace.js",
              "core-js/modules/es.weak-map.js",
              "core-js/modules/web.dom-collections.iterator.js",
              "lodash.debounce",
              "lodash.memoize",
              "lodash.throttle",
              "rxjs/internal/operators",
              "moment",
              "lottie-web",
              "ngx-lightbox",
              "ngx-masonry",
              "leaflet",
              "sweetalert2",
              "lodash",
              "devexpress",
              "devexpress-gantt",
              "devextreme-quill",
              "jszip",
              "@ckeditor/ckeditor5-build-classic",
              "ngx-csv/ngx-csv",
              "dropzone",
              "file-saver"
           ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "11mb",
                  "maximumError": "11mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "velzon:build:production"
            },
            "development": {
              "buildTarget": "velzon:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "velzon:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
           
              "src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    }
  }
}
